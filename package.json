{
  "name": "n8n-nodes-syncmate",
  "version": "1.0.9",
  "description": "Send WhatsApp messages or media using SyncMate Assistro API.",
  "keywords": [
    "n8n-community-node-package",
    "whatsapp",
    "assistro",
    "syncmate",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/heratshah-collab/n8n-nodes-syncmate#readme",
  "author": {
    "name": "Assistro Technologies",
    "email": "support@assistro.co"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heratshah-collab/n8n-nodes-syncmate.git"
  },
  "engines": {
    "node": ">=18.10.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/WhatsAuto/WhatsAuto.node.js"
    ],
    "credentials": [
      "dist/credentials/AssistroOAuth2Api.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0",
    "n8n-core": ">=1.0.0"
  }
}